-- Midnight Chasers: Auto Farm & Delivery
-- Optimized for Delta Executor with clean GUI
-- Dark flat theme, draggable, with minimize/close

if not game:IsLoaded() then
    game.Loaded:Wait()
end

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local RunService = game:GetService("RunService")
local TweenService = game:GetService("TweenService")
local VirtualUser = game:GetService("VirtualUser")
local CoreGui = game:GetService("CoreGui")
local UserInputService = game:GetService("UserInputService")

-- Anti-AFK Setup
task.spawn(function()
    warn("Anti-AFK initialized")
    LocalPlayer.Idled:Connect(function()
        VirtualUser:CaptureController()
        VirtualUser:ClickButton2(Vector2.new())
    end)
end)

-- Initial workspace streaming optimization
task.spawn(function()
    for _, v in pairs(workspace:GetDescendants()) do
        if v.ClassName == "Model" then
            pcall(function()
                LocalPlayer:RequestStreamAroundAsync(v.WorldPivot.Position, 3)
            end)
            task.wait(0.05)
        end
    end
end)

-- Configuration variables
local config = {
    autoFarm = false,
    autoDeliver = false,
    speed = 230,
    groundPart = nil,
    currentTween = nil,
    cancelProximity = false
}

-- Core Functions
local function reachedMax(vehicle)
    local seatCount = 0
    for _, v in pairs(vehicle:GetDescendants()) do
        if v:IsA("VehicleSeat") or v:IsA("Seat") then
            seatCount = seatCount + 1
        end
    end
    seatCount = seatCount * 5
    
    local gifts = vehicle:FindFirstChild("Gifts")
    if gifts and #gifts:GetChildren() >= seatCount then
        return true
    end
    return false
end

local function hideWorkspaceObjects()
    local backupFolder = game:GetService("ReplicatedStorage"):FindFirstChild("mrbackupfolder")
    if not backupFolder then
        backupFolder = Instance.new("Folder")
        backupFolder.Name = "mrbackupfolder"
        backupFolder.Parent = game:GetService("ReplicatedStorage")
    end
    
    for _, v in pairs(workspace:GetChildren()) do
        if (v:IsA("Model") and not string.find(v.Name:lower(), LocalPlayer.Name:lower())) or
           (v:IsA("Folder") and v.Name ~= "Gift" and v.Name ~= "") or
           v:IsA("MeshPart") then
            v.Parent = backupFolder
        end
        task.wait(0.01)
    end
end

local function restoreWorkspaceObjects()
    local backupFolder = game:GetService("ReplicatedStorage"):FindFirstChild("mrbackupfolder")
    if backupFolder then
        for _, v in pairs(backupFolder:GetChildren()) do
            v.Parent = workspace
            task.wait(0.01)
        end
    end
end

local function ensureGroundPart()
    if not config.groundPart then
        local ground = Instance.new("Part")
        ground.Name = "AutoFarmGround"
        ground.Size = Vector3.new(1000, 10, 1000)
        ground.Anchored = true
        ground.CanCollide = false
        ground.Transparency = 1
        ground.Parent = workspace
        config.groundPart = ground
    end
end

local function tweenToPosition(vehicle, targetCFrame, speed)
    if not vehicle or not vehicle.PrimaryPart then return end
    
    local startCFrame = vehicle:GetPrimaryPartCFrame()
    local distance = (startCFrame.Position - targetCFrame.Position).Magnitude
    local tweenTime = distance / speed
    
    local cframeValue = Instance.new("CFrameValue")
    cframeValue.Value = startCFrame
    
    local connection
    connection = cframeValue.Changed:Connect(function()
        if not vehicle or not vehicle.PrimaryPart then
            connection:Disconnect()
            return
        end
        
        local currentPos = cframeValue.Value.Position
        if config.groundPart then
            config.groundPart.Position = currentPos - Vector3.new(0, 14, 0)
        end
        
        local targetPos = targetCFrame.Position
        local lookVector = (targetPos - currentPos).Unit
        local newCFrame = CFrame.new(currentPos, currentPos + lookVector)
        
        vehicle:PivotTo(newCFrame)
        vehicle.PrimaryPart.AssemblyLinearVelocity = lookVector * speed
    end)
    
    local tweenInfo = TweenInfo.new(tweenTime, Enum.EasingStyle.Linear)
    local tween = TweenService:Create(cframeValue, tweenInfo, {Value = targetCFrame})
    
    config.currentTween = tween
    tween:Play()
    
    tween.Completed:Wait()
    connection:Disconnect()
    cframeValue:Destroy()
end

local function tweenToLocation(vehicle, location, player)
    if not vehicle or not vehicle.PrimaryPart or not location then return end
    
    config.cancelProximity = false
    local targetPosition = location.WorldPivot.Position + Vector3.new(0, 5, 0)
    
    repeat
        local speed = config.speed
        if config.cancelProximity then
            speed = 50
        end
        
        local distance = (player.Character.HumanoidRootPart.Position - targetPosition).Magnitude
        local tweenTime = distance / speed
        
        local cframeValue = Instance.new("CFrameValue")
        cframeValue.Value = vehicle:GetPrimaryPartCFrame()
        
        local connection
        connection = cframeValue.Changed:Connect(function()
            if not vehicle or not vehicle.PrimaryPart then
                connection:Disconnect()
                return
            end
            
            local currentPos = cframeValue.Value.Position
            local playerDistance = (player.Character.HumanoidRootPart.Position - targetPosition).Magnitude
            
            if playerDistance > 100 then
                if config.groundPart then
                    config.groundPart.Position = currentPos - Vector3.new(0, 14, 0)
                end
                
                local lookVector = (targetPosition - currentPos).Unit
                local newCFrame = CFrame.new(currentPos, currentPos + lookVector)
                vehicle:PivotTo(newCFrame)
                vehicle.PrimaryPart.AssemblyLinearVelocity = lookVector * speed
                
            elseif playerDistance < 100 and playerDistance > 10 then
                if not config.cancelProximity then
                    config.cancelProximity = true
                    if config.currentTween then
                        config.currentTween:Cancel()
                    end
                end
                
                if config.groundPart then
                    config.groundPart.Position = currentPos - Vector3.new(0, 8, 0)
                end
                
                local targetXZ = Vector3.new(targetPosition.X, vehicle.PrimaryPart.CFrame.Y, targetPosition.Z)
                local lookVector = (targetXZ - currentPos).Unit
                local newCFrame = CFrame.new(currentPos, currentPos + lookVector)
                vehicle:PivotTo(newCFrame)
                vehicle.PrimaryPart.AssemblyLinearVelocity = lookVector * 20
                
            elseif playerDistance < 5 then
                local lookAt = vehicle.PrimaryPart.CFrame * CFrame.new(0, 0, -10000)
                local newPos = Vector3.new(targetPosition.X, config.groundPart.CFrame.Y + 7, targetPosition.Z)
                vehicle:PivotTo(CFrame.new(newPos, lookAt.Position))
                vehicle.PrimaryPart.AssemblyLinearVelocity = Vector3.zero
            end
        end)
        
        local tweenInfo = TweenInfo.new(tweenTime, Enum.EasingStyle.Linear)
        local tween = TweenService:Create(cframeValue, tweenInfo, {Value = CFrame.new(targetPosition)})
        
        config.currentTween = tween
        tween:Play()
        tween.Completed:Wait()
        
        connection:Disconnect()
        cframeValue:Destroy()
        
        task.wait(0.1)
    until not location:FindFirstChild("Highlight") or not config.autoDeliver
    
    config.cancelProximity = false
end

-- GUI Creation
local function createGUI()
    -- Main ScreenGui
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "MidnightChasersGUI"
    screenGui.Parent = CoreGui
    
    -- Main Frame
    local mainFrame = Instance.new("Frame")
    mainFrame.Name = "MainFrame"
    mainFrame.Size = UDim2.new(0, 350, 0, 280)
    mainFrame.Position = UDim2.new(0.5, -175, 0.5, -140)
    mainFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    mainFrame.BorderSizePixel = 0
    mainFrame.ClipsDescendants = true
    mainFrame.Parent = screenGui
    
    -- Title Bar
    local titleBar = Instance.new("Frame")
    titleBar.Name = "TitleBar"
    titleBar.Size = UDim2.new(1, 0, 0, 35)
    titleBar.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
    titleBar.BorderSizePixel = 0
    titleBar.Parent = mainFrame
    
    local titleLabel = Instance.new("TextLabel")
    titleLabel.Name = "TitleLabel"
    titleLabel.Size = UDim2.new(1, -70, 1, 0)
    titleLabel.Position = UDim2.new(0, 10, 0, 0)
    titleLabel.BackgroundTransparency = 1
    titleLabel.Text = "Midnight Chasers Auto Farm"
    titleLabel.TextColor3 = Color3.fromRGB(220, 220, 220)
    titleLabel.Font = Enum.Font.GothamMedium
    titleLabel.TextSize = 14
    titleLabel.TextXAlignment = Enum.TextXAlignment.Left
    titleLabel.Parent = titleBar
    
    -- Close Button
    local closeButton = Instance.new("TextButton")
    closeButton.Name = "CloseButton"
    closeButton.Size = UDim2.new(0, 35, 0, 35)
    closeButton.Position = UDim2.new(1, -35, 0, 0)
    closeButton.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
    closeButton.BorderSizePixel = 0
    closeButton.Text = "X"
    closeButton.TextColor3 = Color3.fromRGB(220, 220, 220)
    closeButton.Font = Enum.Font.GothamBold
    closeButton.TextSize = 14
    closeButton.Parent = titleBar
    
    closeButton.MouseButton1Click:Connect(function()
        screenGui:Destroy()
        config.autoFarm = false
        config.autoDeliver = false
        restoreWorkspaceObjects()
    end)
    
    closeButton.MouseEnter:Connect(function()
        closeButton.BackgroundColor3 = Color3.fromRGB(220, 50, 50)
    end)
    
    closeButton.MouseLeave:Connect(function()
        closeButton.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
    end)
    
    -- Minimize Button
    local minimizeButton = Instance.new("TextButton")
    minimizeButton.Name = "MinimizeButton"
    minimizeButton.Size = UDim2.new(0, 35, 0, 35)
    minimizeButton.Position = UDim2.new(1, -70, 0, 0)
    minimizeButton.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
    minimizeButton.BorderSizePixel = 0
    minimizeButton.Text = "â€“"
    minimizeButton.TextColor3 = Color3.fromRGB(220, 220, 220)
    minimizeButton.Font = Enum.Font.GothamBold
    minimizeButton.TextSize = 16
    minimizeButton.Parent = titleBar
    
    local minimized = false
    local originalSize = mainFrame.Size
    
    minimizeButton.MouseButton1Click:Connect(function()
        minimized = not minimized
        if minimized then
            mainFrame.Size = UDim2.new(originalSize.X.Scale, originalSize.X.Offset, 0, 35)
        else
            mainFrame.Size = originalSize
        end
    end)
    
    minimizeButton.MouseEnter:Connect(function()
        minimizeButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    end)
    
    minimizeButton.MouseLeave:Connect(function()
        minimizeButton.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
    end)
    
    -- Content Frame
    local contentFrame = Instance.new("Frame")
    contentFrame.Name = "ContentFrame"
    contentFrame.Size = UDim2.new(1, 0, 1, -35)
    contentFrame.Position = UDim2.new(0, 0, 0, 35)
    contentFrame.BackgroundTransparency = 1
    contentFrame.Parent = mainFrame
    
    -- Speed Input
    local speedContainer = Instance.new("Frame")
    speedContainer.Name = "SpeedContainer"
    speedContainer.Size = UDim2.new(1, -20, 0, 40)
    speedContainer.Position = UDim2.new(0, 10, 0, 15)
    speedContainer.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    speedContainer.BorderSizePixel = 0
    speedContainer.Parent = contentFrame
    
    local speedLabel = Instance.new("TextLabel")
    speedLabel.Name = "SpeedLabel"
    speedLabel.Size = UDim2.new(0.5, 0, 1, 0)
    speedLabel.Position = UDim2.new(0, 10, 0, 0)
    speedLabel.BackgroundTransparency = 1
    speedLabel.Text = "Vehicle Speed:"
    speedLabel.TextColor3 = Color3.fromRGB(220, 220, 220)
    speedLabel.Font = Enum.Font.GothamMedium
    speedLabel.TextSize = 14
    speedLabel.TextXAlignment = Enum.TextXAlignment.Left
    speedLabel.Parent = speedContainer
    
    local speedBox = Instance.new("TextBox")
    speedBox.Name = "SpeedBox"
    speedBox.Size = UDim2.new(0.4, -10, 0.6, 0)
    speedBox.Position = UDim2.new(0.6, 0, 0.2, 0)
    speedBox.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    speedBox.BorderSizePixel = 0
    speedBox.Text = tostring(config.speed)
    speedBox.TextColor3 = Color3.fromRGB(220, 220, 220)
    speedBox.Font = Enum.Font.GothamMedium
    speedBox.TextSize = 14
    speedBox.PlaceholderText = "Speed"
    speedBox.Parent = speedContainer
    
    speedBox.FocusLost:Connect(function()
        local num = tonumber(speedBox.Text)
        if num and num > 0 then
            config.speed = num
        else
            speedBox.Text = tostring(config.speed)
        end
    end)
    
    -- Auto Farm Toggle
    local farmToggle = Instance.new("TextButton")
    farmToggle.Name = "FarmToggle"
    farmToggle.Size = UDim2.new(1, -20, 0, 45)
    farmToggle.Position = UDim2.new(0, 10, 0, 70)
    farmToggle.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
    farmToggle.BorderSizePixel = 0
    farmToggle.Text = "Auto Farm: OFF"
    farmToggle.TextColor3 = Color3.fromRGB(220, 220, 220)
    farmToggle.Font = Enum.Font.GothamMedium
    farmToggle.TextSize = 14
    farmToggle.Parent = contentFrame
    
    -- Auto Deliver Toggle
    local deliverToggle = Instance.new("TextButton")
    deliverToggle.Name = "DeliverToggle"
    deliverToggle.Size = UDim2.new(1, -20, 0, 45)
    deliverToggle.Position = UDim2.new(0, 10, 0, 125)
    deliverToggle.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
    deliverToggle.BorderSizePixel = 0
    deliverToggle.Text = "Auto Deliver: OFF"
    deliverToggle.TextColor3 = Color3.fromRGB(220, 220, 220)
    deliverToggle.Font = Enum.Font.GothamMedium
    deliverToggle.TextSize = 14
    deliverToggle.Parent = contentFrame
    
    -- Status Label
    local statusLabel = Instance.new("TextLabel")
    statusLabel.Name = "StatusLabel"
    statusLabel.Size = UDim2.new(1, -20, 0, 40)
    statusLabel.Position = UDim2.new(0, 10, 0, 180)
    statusLabel.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    statusLabel.BorderSizePixel = 0
    statusLabel.Text = "Status: Ready"
    statusLabel.TextColor3 = Color3.fromRGB(180, 220, 180)
    statusLabel.Font = Enum.Font.GothamMedium
    statusLabel.TextSize = 14
    statusLabel.Parent = contentFrame
    
    -- Draggable functionality
    local dragging = false
    local dragInput, dragStart, startPos
    
    local function updateStatus(text, color)
        statusLabel.Text = "Status: " .. text
        statusLabel.TextColor3 = color or Color3.fromRGB(180, 220, 180)
    end
    
    titleBar.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            dragStart = input.Position
            startPos = mainFrame.Position
            
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false
                end
            end)
        end
    end)
    
    titleBar.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement then
            dragInput = input
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if dragging and input == dragInput then
            local delta = input.Position - dragStart
            mainFrame.Position = UDim2.new(
                startPos.X.Scale,
                startPos.X.Offset + delta.X,
                startPos.Y.Scale,
                startPos.Y.Offset + delta.Y
            )
        end
    end)
    
    -- Toggle functionality
    farmToggle.MouseButton1Click:Connect(function()
        config.autoFarm = not config.autoFarm
        farmToggle.Text = "Auto Farm: " .. (config.autoFarm and "ON" or "OFF")
        farmToggle.BackgroundColor3 = config.autoFarm and Color3.fromRGB(60, 120, 60) or Color3.fromRGB(45, 45, 45)
        
        if config.autoFarm then
            config.autoDeliver = false
            deliverToggle.Text = "Auto Deliver: OFF"
            deliverToggle.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
            
            updateStatus("Starting Auto Farm...")
            task.spawn(function()
                hideWorkspaceObjects()
                ensureGroundPart()
                
                local farmPoints = {
                    CFrame.new(105.419128, -26.0098934, 7965.37988, -3.36170197e-05, 0.951051414, -0.309032798, -1, -3.36170197e-05, 5.31971455e-06, -5.31971455e-06, 0.309032798, 0.951051414),
                    CFrame.new(2751.86499, -26.0098934, 3694.63354, 3.34978104e-05, 0.951051414, -0.309032798, -1, 3.34978104e-05, -5.31971455e-06, 5.31971455e-06, 0.309032798, 0.951051414),
                    CFrame.new(-8821.48438, -26.0098934, 2042.49939, -3.36170197e-05, 0.951051414, -0.309032798, -1, -3.36170197e-05, 5.31971455e-06, -5.31971455e-06, 0.309032798, 0.951051414),
                    CFrame.new(-6408.62109, -26.0098934, -727.765198, 3.34978104e-05, 0.951051414, -0.309032798, -1, 3.34978104e-05, -5.31971455e-06, 5.31971455e-06, 0.309032798, 0.951051414),
                    CFrame.new(-6099.79639, -26.00989345, -1027.94556, -3.36170197e-05, 0.951051414, -0.309032798, -1, -3.36170197e-05, 5.31971455e-06, -5.31971455e-06, 0.309032798, 0.951051414),
                    CFrame.new(-6066.70068, -26.0098934, 493.255524, 3.34978104e-05, 0.951051414, -0.309032798, -1, 3.34978104e-05, -5.31971455e-06, 5.31971455e-06, 0.309032798, 0.951051414),
                    CFrame.new(132.786133, -26.0098934, 15.2286377, 3.34978104e-05, 0.951051414, -0.309032798, -1, 3.34978104e-05, -5.31971455e-06, 5.31971455e-06, 0.309032798, 0.951051414),
                    CFrame.new(-7692.85449, -26.0098934, -4668.61963, -3.36170197e-05, 0.951051414, -0.309032798, -1, -3.36170197e-05, 5.31971455e-06, -5.31971455e-06, 0.309032798, 0.951051414),
                    CFrame.new(4887.24609, -26.0098934, 1222.96826, -3.36170197e-05, 0.951051414, -0.309032798, -1, -3.36170197e-05, 5.31971455e-06, -5.31971455e-06, 0.309032798, 0.951051414)
                }
                
                while config.autoFarm and wait(0.1) do
                    local char = LocalPlayer.Character
                    if not char then continue end
                    
                    local humanoid = char:FindFirstChild("Humanoid")
                    if not humanoid then continue end
                    
                    local seat = humanoid.SeatPart
                    if not seat then
                        updateStatus("Waiting for vehicle...", Color3.fromRGB(220, 220, 100))
                        continue
                    end
                    
                    local vehicle = seat.Parent
                    if not vehicle then continue end
                    
                    local primary = vehicle:FindFirstChild("Body")
                    if primary then
                        primary = primary:FindFirstChild("#Weight")
                    end
                    
                    if primary then
                        vehicle.PrimaryPart = primary
                        updateStatus("Farming...", Color3.fromRGB(100, 200, 100))
                        
                        for _, point in pairs(farmPoints) do
                            if not config.autoFarm then break end
                            tweenToPosition(vehicle, point + Vector3.new(0, 5, 0), config.speed)
                            task.wait(0.5)
                        end
                    else
                        updateStatus("No vehicle weight found", Color3.fromRGB(220, 100, 100))
                    end
                end
                
                if not config.autoFarm then
                    updateStatus("Auto Farm stopped", Color3.fromRGB(220, 220, 100))
                    if config.currentTween then
                        config.currentTween:Cancel()
                    end
                    restoreWorkspaceObjects()
                end
            end)
        else
            updateStatus("Auto Farm stopped", Color3.fromRGB(220, 220, 100))
            if config.currentTween then
                config.currentTween:Cancel()
            end
            restoreWorkspaceObjects()
        end
    end)
    
    deliverToggle.MouseButton1Click:Connect(function()
        config.autoDeliver = not config.autoDeliver
        deliverToggle.Text = "Auto Deliver: " .. (config.autoDeliver and "ON" or "OFF")
        deliverToggle.BackgroundColor3 = config.autoDeliver and Color3.fromRGB(60, 100, 120) or Color3.fromRGB(45, 45, 45)
        
        if config.autoDeliver then
            config.autoFarm = false
            farmToggle.Text = "Auto Farm: OFF"
            farmToggle.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
            
            updateStatus("Starting Auto Deliver...")
            task.spawn(function()
                hideWorkspaceObjects()
                ensureGroundPart()
                
                while config.autoDeliver and task.wait(0.1) do
                    local char = LocalPlayer.Character
                    if not char then continue end
                    
                    local humanoid = char:FindFirstChild("Humanoid")
                    if not humanoid then continue end
                    
                    local seat = humanoid.SeatPart
                    if not seat then
                        updateStatus("Waiting for vehicle...", Color3.fromRGB(220, 220, 100))
                        continue
                    end
                    
                    local vehicle = seat.Parent
                    if not vehicle then continue end
                    
                    local primary = vehicle:FindFirstChild("Body")
                    if primary then
                        primary = primary:FindFirstChild("#Weight")
                    end
                    
                    if primary then
                        vehicle.PrimaryPart = primary
                        
                        -- Find nearest delivery location
                        local nearestLocation = nil
                        local shortestDistance = math.huge
                        
                        for _, obj in pairs(workspace:GetChildren()) do
                            if obj.Name == "" and obj:FindFirstChild("Highlight") then
                                local distance = (char.HumanoidRootPart.Position - obj.WorldPivot.Position).Magnitude
                                if distance < shortestDistance then
                                    shortestDistance = distance
                                    nearestLocation = obj
                                end
                            end
                        end
                        
                        if nearestLocation then
                            updateStatus("Delivering...", Color3.fromRGB(100, 180, 220))
                            tweenToLocation(vehicle, nearestLocation, LocalPlayer)
                        else
                            -- Go to gift pickup if full
                            local giftPickup = workspace:FindFirstChild("GiftPickup")
                            if giftPickup and reachedMax(vehicle) then
                                updateStatus("Returning gifts...", Color3.fromRGB(180, 100, 220))
                                vehicle:PivotTo(giftPickup.WorldPivot * CFrame.new(0, 5, 0))
                                vehicle.PrimaryPart.AssemblyLinearVelocity = Vector3.zero
                                task.wait(2)
                            else
                                updateStatus("No delivery points found", Color3.fromRGB(220, 100, 100))
                                task.wait(1)
                            end
                        end
                    else
                        updateStatus("No vehicle weight found", Color3.fromRGB(220, 100, 100))
                    end
                end
                
                if not config.autoDeliver then
                    updateStatus("Auto Deliver stopped", Color3.fromRGB(220, 220, 100))
                    if config.currentTween then
                        config.currentTween:Cancel()
                    end
                    restoreWorkspaceObjects()
                end
            end)
        else
            updateStatus("Auto Deliver stopped", Color3.fromRGB(220, 220, 100))
            if config.currentTween then
                config.currentTween:Cancel()
            end
            restoreWorkspaceObjects()
        end
    end)
    
    -- Button hover effects
    farmToggle.MouseEnter:Connect(function()
        if not config.autoFarm then
            farmToggle.BackgroundColor3 = Color3.fromRGB(55, 55, 55)
        end
    end)
    
    farmToggle.MouseLeave:Connect(function()
        if not config.autoFarm then
            farmToggle.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
        end
    end)
    
    deliverToggle.MouseEnter:Connect(function()
        if not config.autoDeliver then
            deliverToggle.BackgroundColor3 = Color3.fromRGB(55, 55, 55)
        end
    end)
    
    deliverToggle.MouseLeave:Connect(function()
        if not config.autoDeliver then
            deliverToggle.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
        end
    end)
    
    return screenGui
end

-- Initialize
task.wait(1)
createGUI()
warn("Midnight Chasers GUI Loaded - Delta Executor Compatible")
