AutoFarmButton.Text = "START AUTO FARM"
AutoFarmButton.Size = UDim2.new(0.9, 0, 0, 45)
AutoFarmButton.Position = UDim2.new(0.05, 0, 0.35, 0)
AutoFarmButton.BackgroundColor3 = Color3.fromRGB(70, 120, 70)
AutoFarmButton.TextColor3 = Color3.fromRGB(255, 255, 255)
