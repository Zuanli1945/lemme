UICorner6.Parent = CloseButton

-- Drag GUI
local UserInputService = game:GetService("UserInputService")
local dragging = false
local dragInput, dragStart, startPos

local function updateInput(input)
    local delta = input.Position - dragStart
    MainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

MainFrame.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = MainFrame.Position
        
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

MainFrame.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement then
        dragInput = input
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if dragging and input == dragInput then
        updateInput(input)
    end
end)

-- Functions
function UpdateStatus(text, color)
    StatusLabel.Text = "Status: " .. text
    StatusLabel.TextColor3 = color
end

function GetVehicle()
    if not Character or not Humanoid then return nil end
    local seat = Humanoid.SeatPart
    if not seat then return nil end
    return seat.Parent
end

function StopAll()
    AutoFarm = false
    AutoDeliver = false
    
    if CurrentTween then
        CurrentTween:Cancel()
        CurrentTween = nil
    end
    
    UpdateStatus("Stopped", Color3.fromRGB(255, 100, 100))
    AutoFarmButton.Text = "START AUTO FARM"
    AutoFarmButton.BackgroundColor3 = Color3.fromRGB(70, 120, 70)
    AutoDeliverButton.Text = "START AUTO DELIVER"
    AutoDeliverButton.BackgroundColor3 = Color3.fromRGB(70, 70, 120)
    
    -- Stop vehicle movement
    local vehicle = GetVehicle()
    if vehicle and vehicle.PrimaryPart then
        vehicle.PrimaryPart.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
    end
end

function MoveToPosition(targetCFrame)
    local vehicle = GetVehicle()
    if not vehicle or not vehicle.PrimaryPart then return false end
    
    local startPos = vehicle.PrimaryPart.Position
    local endPos = targetCFrame.Position
    local distance = (startPos - endPos).Magnitude
    local duration = distance / Speed
    
    local startTime = tick()
    
    while tick() - startTime < duration and (AutoFarm or AutoDeliver) do
        if not vehicle or not vehicle.PrimaryPart then break end
        
        local elapsed = tick() - startTime
        local progress = math.min(elapsed / duration, 1)
        
        -- Calculate new position
        local newPos = startPos:Lerp(endPos, progress)
        local lookVector = (endPos - newPos).Unit
        
        -- Move vehicle
        local newCFrame = CFrame.new(newPos, newPos + lookVector)
        vehicle:PivotTo(newCFrame)
        
        -- Add velocity
        vehicle.PrimaryPart.AssemblyLinearVelocity = lookVector * Speed
        
        RunService.Heartbeat:Wait()
    end
    
    -- Stop at destination
    if vehicle and vehicle.PrimaryPart then
        vehicle.PrimaryPart.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
    end
    
    return true
end

-- Farming Routes (adjust these coordinates based on your game)
local FarmRoutes = {
    CFrame.new(100, 10, 8000),
    CFrame.new(2800, 10, 3700),
    CFrame.new(-8800, 10, 2000),
    CFrame.new(-6400, 10, -700),
    CFrame.new(-6100, 10, -1000),
    CFrame.new(-6000, 10, 500),
    CFrame.new(150, 10, 20),
    CFrame.new(-7700, 10, -4700),
    CFrame.new(4900, 10, 1200)
}

-- Auto Farm Function
AutoFarmButton.MouseButton1Click:Connect(function()
    if not AutoFarm then
        -- Start Auto Farm
        AutoFarm = true
        AutoDeliver = false
        
        AutoFarmButton.Text = "STOP AUTO FARM"
        AutoFarmButton.BackgroundColor3 = Color3.fromRGB(120, 70, 70)
        AutoDeliverButton.Text = "START AUTO DELIVER"
        AutoDeliverButton.BackgroundColor3 = Color3.fromRGB(70, 70, 120)
        
        UpdateStatus("Farming...", Color3.fromRGB(0, 255, 0))
        
        -- Start farming in a separate thread
        task.spawn(function()
            while AutoFarm do
                local vehicle = GetVehicle()
                if not vehicle then
                    UpdateStatus("No vehicle!", Color3.fromRGB(255, 255, 0))
                    task.wait(1)
                    continue
                end
                
                for _, route in ipairs(FarmRoutes) do
                    if not AutoFarm then break end
                    
                    UpdateStatus("Moving to point...", Color3.fromRGB(100, 200, 255))
                    MoveToPosition(route)
                    
                    -- Wait a bit at each point
                    for i = 1, 10 do
                        if not AutoFarm then break end
                        task.wait(0.5)
                    end
                end
                
                if AutoFarm then
                    UpdateStatus("Route complete, restarting...", Color3.fromRGB(0, 255, 255))
                end
            end
        end)
    else
        -- Stop Auto Farm
        StopAll()
    end
end)

-- Auto Deliver Function
AutoDeliverButton.MouseButton1Click:Connect(function()
    if not AutoDeliver then
        -- Start Auto Deliver
        AutoDeliver = true
        AutoFarm = false
        
        AutoDeliverButton.Text = "STOP AUTO DELIVER"
        AutoDeliverButton.BackgroundColor3 = Color3.fromRGB(120, 70, 120)
        AutoFarmButton.Text = "START AUTO FARM"
        AutoFarmButton.BackgroundColor3 = Color3.fromRGB(70, 120, 70)
        
        UpdateStatus("Delivering...", Color3.fromRGB(0, 200, 0))
        
        -- Start delivering in a separate thread
        task.spawn(function()
            while AutoDeliver do
                local vehicle = GetVehicle()
                if not vehicle then
                    UpdateStatus("No vehicle!", Color3.fromRGB(255, 255, 0))
                    task.wait(1)
                    continue
                end
                
                -- Find gift pickup
                local giftPickup = Workspace:FindFirstChild("GiftPickup")
                if giftPickup then
                    UpdateStatus("Going to gift pickup...", Color3.fromRGB(200, 100, 255))
                    MoveToPosition(giftPickup.CFrame + Vector3.new(0, 5, 0))
                    task.wait(3) -- Wait for pickup
                else
                    -- Find any gift location
                    for _, obj in pairs(Workspace:GetChildren()) do
                        if obj.Name == "" and obj:FindFirstChild("Highlight") then
                            UpdateStatus("Found gift location!", Color3.fromRGB(255, 200, 0))
                            MoveToPosition(obj:GetPivot() + Vector3.new(0, 5, 0))
                            task.wait(2) -- Wait to collect
                            break
                        end
                    end
                end
                
                task.wait(0.5)
            end
        end)
    else
        -- Stop Auto Deliver
        StopAll()
    end
end)

-- Stop Button
StopButton.MouseButton1Click:Connect(function()
    StopAll()
end)

-- Speed Slider
SpeedSlider.FocusLost:Connect(function(enterPressed)
    if enterPressed then
        local newSpeed = tonumber(SpeedSlider.Text)
        if newSpeed and newSpeed > 0 then
            Speed = newSpeed
            SpeedLabel.Text = "Speed: " .. Speed
            UpdateStatus("Speed set to " .. Speed, Color3.fromRGB(100, 255, 100))
        else
            SpeedSlider.Text = tostring(Speed)
        end
    end
end)

-- Close Button
CloseButton.MouseButton1Click:Connect(function()
    StopAll()
    ScreenGui:Destroy()
end)

-- Initial Stream Request (optional)
task.spawn(function()
    task.wait(3)
    for _, model in pairs(Workspace:GetDescendants()) do
        if model:IsA("Model") and model.PrimaryPart then
            pcall(function()
                Player:RequestStreamAroundAsync(model.PrimaryPart.Position, 3)
            end)
            task.wait(0.05)
        end
    end
    print("Streaming complete!")
end)

print("Midnight Chasers Farm Script Loaded!")
print("Enter a vehicle and press a button to start!")
UpdateStatus("Ready - Enter Vehicle", Color3.fromRGB(0, 255, 0))
